<config xmlns="http://tail-f.com/ns/config/1.0">
  <environments xmlns="https://curity.se/ns/conf/base">
    <environment>
      <base-url>https://login.example.com:8443</base-url>
      <admin-service>
        <http>
          <ssl-server-keystore>default-admin-ssl-key</ssl-server-keystore>
          <web-ui/>
          <restconf>
          </restconf>
        </http>
      </admin-service>
      <services>
        <zones>
          <default-zone>
            <symmetric-key>data:text/plain;aes,v:S.VUtOdFhBZ242dnFxdHk3cg==.NID_dl33J7NUXxwIs-VZRQ==.5CZONkdZCxfP7zT5VukAMfpNhspuhCcVIbNRmFTKw3E1c_-QmeC7TcdZMmX2cXbeKXb6reOGF4GeBLebpsmJ0QwNiZcHJE2hDihN_7_UET0=.oiYcRIgfXbVWaHEnSp6VtRBmK796Z9qq5UCDq3RwgDA=</symmetric-key>
